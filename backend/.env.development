# Development Environment - Uses Mock Services
# No GPU server required

# LLM Configuration (Mock)
LLM_API_URL=http://localhost:8001/v1/chat/completions
LLM_MODEL=mock-llama3-8b-instruct

# Embedding API Configuration (Mock)
EMBEDDING_API_URL=http://localhost:8001/v1/embeddings
EMBEDDING_MODEL=mock-all-MiniLM-L6-v2
EMBEDDING_DIMENSION=384

# Environment
ENVIRONMENT=development
USE_MOCK_SERVICES=true

# Data Paths
UPLOAD_DIR=data/uploads
CHROMA_DIR=data/chroma
NOTEBOOKS_FILE=data/notebooks.json

# Server
HOST=0.0.0.0
PORT=8000
